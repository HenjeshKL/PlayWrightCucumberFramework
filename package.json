  "description": "",
  "devDependencies": {
    "@playwright/test": "^1.51.1",
    "@types/node": "^22.12.0",
    "allure-playwright": "^3.2.1"
  },
  "dependencies": {
    "@cucumber/cucumber": "^11.2.0",
    "exceljs": "^4.4.0"
  }
}